res_RW1 <- inla(formula_RW1, family = "gaussian", data = test_data,
control.compute = list(cpo = TRUE),
control.family = list(hyper = list(prec =
list(initial = log(P), fixed = TRUE))))
LS_RW1 <- -mean(log(res_RW1$cpo$cpo))
