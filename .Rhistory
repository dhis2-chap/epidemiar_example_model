list(initial = log(P), fixed = TRUE))))
LS_ARW1 <- -mean(log(res_ARW1$cpo$cpo))
RMSE_ARW1 <- RMSE(df_eta[, i], res_ARW1$summary.fitted.values$mean)
eval_df[i, ] <- c(RMSE_RW1, LS_RW1, RMSE_ARW1, LS_ARW1)
}
